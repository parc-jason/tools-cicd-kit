{
  "name": "generator-cicd-tools",
  "version": "0.0.0",
  "description": "",
  "homepage": "",
  "author": {
    "name": "parc-jason",
    "email": "jsherman@parcsystems.ca",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^6.6.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^2.1.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly",
      "_": false
    },
    "parserOptions": {
      "ecmaVersion": 9
    },
    "rules": {
      "eol-last": [
        "error",
        "always"
      ],
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    },
    "plugins": []
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": "https://github.com/parc-jason/tools-cicd-kit.git",
  "license": "Apache-2.0"
}
